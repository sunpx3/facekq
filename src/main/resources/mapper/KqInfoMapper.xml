<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.edu.bucm.mapper.KqInfoMapper">
    <select id="findkqinfos"  resultType="cn.edu.bucm.mapper.KqInfo">
        select * from KQ_INFO
    </select>

    <select id="findkqinfobyxgh"  parameterType="java.lang.String"  resultType="cn.edu.bucm.mapper.KqInfo">
        SELECT * FROM KQ_INFO  WHERE xgh= #{xgh}
    </select>

    <select id="count" resultType="int">
        select count(*) from KQ_INFO t where xgh= #{xgh}
    </select>
</mapper>

